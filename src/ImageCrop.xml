<?xml version="1.0" encoding="utf-8" ?>
<widget id="com.mendix.widget.custom.imagecrop.ImageCrop" needsEntityContext="true" offlineCapable="true" xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../xsd/widget.xsd">
    <name>Image crop</name>
    <description>A responsive image cropping tool for Mendix</description>
    <icon></icon>
     <properties>
        <property key="positionX" type="integer" defaultValue="0">
            <caption>Position X</caption>
            <category>Appearance</category>
            <description>The X position of initial crop coordinate (in px)</description>
        </property>
        <property key="positionY" type="integer" defaultValue="0">
            <caption>Position Y</caption>
            <category>Appearance</category>
             <description>The Y position of initial crop coordinate (in px)</description>
        </property>
        <property key="minWidth" type="integer" defaultValue="0">
            <caption>Minimum width</caption>
            <category>Appearance</category>
            <description>The initial minimum width that the cropped image can be</description>
        </property>
        <property key="minHeight" type="integer" defaultValue="0">
            <caption>Minimum height</caption>
            <category>Appearance</category>
            <description>The initial minimum height that the cropped image can be</description>
        </property>
        <property key="maxWidth" type="integer" defaultValue="100">
            <caption>Maximum width</caption>
            <category>Appearance</category>
            <description>The maximum width that the image cropper can become</description>
        </property>
        <property key="maxHeight" type="integer" defaultValue="100">
            <caption>Maximum height</caption>
            <category>Appearance</category>
            <description>The maximum height that the image cropper can become</description>
        </property>
    </properties>
</widget>
